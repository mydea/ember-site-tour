{
  "name": "ember-site-tour",
  "version": "2.2.0",
  "description": "An Ember add-on to add site tours based on hopscotch.js to your app.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:one": "ember try:one"
  },
  "repository": "https://github.com/mydea/ember-site-tour",
  "engines": {
    "node": "10.* || >= 12"
  },
  "author": "Francesco Novy <francesconovy@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^3.4.1",
    "broccoli-asset-rev": "~3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.18.0",
    "ember-cli-app-version": "~3.2.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "~0.2.0",
    "ember-cli-inject-live-reload": "~2.0.2",
    "ember-cli-release-tag": "^0.1.5",
    "ember-cli-shims": "~1.2.0",
    "ember-cli-uglify": "~3.0.0",
    "ember-disable-prototype-extensions": "~1.1.3",
    "ember-export-application-global": "~2.0.1",
    "ember-load-initializers": "~2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "~5.2.1",
    "ember-prism": "~0.6.1",
    "ember-qunit": "~4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.18.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.6.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "~8.4.0",
    "eslint-plugin-ember-suave": "^2.0.0",
    "eslint-plugin-node": "~11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.2.0"
  },
  "keywords": [
    "ember-addon",
    "tour",
    "site-tour",
    "hopscotch",
    "hopscotchjs",
    "hopscotch.js"
  ],
  "dependencies": {
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.2",
    "ember-cli-babel": "^7.19.0",
    "ember-cli-htmlbars": "^4.3.1",
    "ember-concurrency": "^1.0.0",
    "hopscotch": "~0.3.1"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://mydea.github.io/ember-site-tour/"
  }
}
